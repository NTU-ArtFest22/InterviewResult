<% include header %>
<div id="result">
  <section class="header">
    <div class="container">
      <img src="/image/vdc-icon.png" class="img-responsive center-block wow fadeInDown" data-wow-duration="2s" alt="">
      <h3 class="wow fadeInUp" data-wow-duration="2s">
      	<%= name %><br>
      	恭喜你錄取紀實創作部 <%=vdcSpecialCase %>
      </h3>
      <p class="message wow fadeInUp" data-wow-duration="2s" data-wow-delay="1s">
        
      </p>
      <a href="/" class="wow fadeInUp" data-wow-duration="2s" data-wow-delay="1s">
        <button class="btn btn-transparent center-block">返回前頁</button>
      </a>
    </div>
  </section>
</div>
<script>
$(document).ready(function(){
//activate wow
  $('#result').css( "display", "block" );
  new WOW().init();
});
</script>
<% include footer %>